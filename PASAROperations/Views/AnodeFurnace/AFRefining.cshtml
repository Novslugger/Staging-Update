                </tr>
                  <tr>
                    <th>&nbsp;</th>
                    <th colspan="2">Refining</th>
                    <th colspan="2">Casting</th>
                </tr>
                 <tr>
                    <th>&nbsp;</th>
                    <th>Start</th>
                    <th>End</th>
                    <th>Start</th>
                    <th>End</th>
                </tr>
                 <tr>
                        <th class="td_left">Date</th>
                    <td id="stdate_refining">&nbsp;</td>
                    <td id="etdate_refining">&nbsp;</td>
                     <td id="stdate_casting">&nbsp;</td>
                    <td id="etdate_casting">&nbsp;</td>
                 
                </tr>
                 <tr>
                        <th class="td_left">Time</th>
                     <td id="sttime_refining">&nbsp;</td>
                     <td id="ettime_refining">&nbsp;</td>
                     <td id="sttime_casting">&nbsp;</td>
                     <td id="ettime_casting">&nbsp;</td>
                </tr>
                <tr>
                        <th class="td_left">Operator</th>
                    <td id="st_roperator">&nbsp;</td>
                    <td id="et_roperator">&nbsp;</td>
                    <td id="st_coperator">&nbsp;</td>
                    <td id="et_coperator">&nbsp;</td>
                </tr>
                <tr>

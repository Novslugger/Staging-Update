@{
    ViewBag.Title = "AF Refining";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

 @Scripts.Render("~/bundles/jquery") 
 @Styles.Render("~/Content/themes/base/css")

<div id="Dialog_AFManPower" style="overflow: hidden;"></div>
<div id="Dialog_AFStage" style="overflow: hidden;"></div>


 @Html.Partial("~/Views/Shared/_AFLotNoSelect.cshtml")

<h6 class="lbl_title">Refining</h6>

<div class="container_div">
<div class="left_div">
    <div class="left_top_div">
        <div>
            <table class="af_tbl">
                <tr>
                        <th colspan="5">ANODE FURNACE MANPOWER
                            <img id="af_manpower_btn" width="20" height="20" src="../Images/red_plus.png" />

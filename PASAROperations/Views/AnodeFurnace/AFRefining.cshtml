                     <td id="et_rsupervisor">&nbsp;</td>
                     <td id="st_csupervisor">&nbsp;</td>
                     <td id="et_csupervisor">&nbsp;</td>
                </tr>

            </table>

        </div>
        
        
    </div>
    <div class="left_bottom_div">
        <div class="left_bottom_div_tbl">
                <table class="af_tbl" width="800">
               <tr>
                        <th class="th_stage_label" rowspan="9">SKIMMING
                            <br />
                            (30 mins.)</th>
                    <th>&nbsp;</th>
                    <th>Start</th>
                    <th>End</th>
                    <th>Endpoint</th>
                    <th>Target</th>
                    <th>Actual</th>
                </tr>
                 <tr>
                        <th class="td_left" id="skim1">1st</th>
                    <td id="st_skim1st">&nbsp;</td>
                    <td id="et_skim1st">&nbsp;</td>
                        <th class="td_left">% Oxygen</th>
                    <td id="skO2_target">&nbsp;</td>
                    <td id="skO2_val">&nbsp;</td>
                </tr>
              <tr>
                        <th class="td_left" id="skim2">2nd</th>
                    <td id="st_skim2nd">&nbsp;</td>
                    <td id="et_skim2nd">&nbsp;</td>
                        <th class="td_left">ppm Sulfur</th>
                        <td id="paramid_140">&nbsp;</td>
                    <td id="skS_val">&nbsp;</td>
                </tr>
              <tr>
                        <th class="td_left" id="skimh">&nbsp;&nbsp;&nbsp;&nbsp;Heat-up&nbsp;&nbsp;&nbsp;&nbsp;</th>
                    <td id="st_skimheatup">&nbsp;</td>
                    <td id="et_skimheatup">&nbsp;</td>
                        <th class="td_left">ppm Lead (Pb)</th>
                    <td id="skPb_target">&nbsp;</td>
                    <td id="skPb_val">&nbsp;</td>
                </tr>
             <tr>
                        <th rowspan="5" class="td_left">Delays</th>
                        <th id="" rowspan="5" colspan="2" class="td_left">
                            @Html.DropDownList("af_delay1", (SelectList)ViewBag.dreason_List1, new { @id = "delay_skim" })

@{
    ViewBag.Title = "_AFanodecharged_edit";
}

<div id="add_lotnum_div">
    <br />
       
        <strong>Date: </strong> 
        <input disabled="disabled" id="select_ancharged_date_edit"  value="@ViewBag.AF_DTime" style=" width: 180px; height: 25px; display: inline; font-size:15px;" type="text" />&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;

        <br>
        <br>
        <strong>Lot no.</strong>
        <div disabled="disabled" style="display: inline; width: 170px; height: 25px; font-size: 15px;">@ViewBag.lotno</div>
        <br>   
        <br>   
        <br>
        <strong>Prov. Weight:</strong>
        <input onkeypress="return NumericOnly(event)" value="@ViewBag.AF_Weight" type="number" id="prov_weight_edit" style=" width: 95px; height: 25px; display: inline;"/>
        <br>
        <br>
        <br>              
        <strong>No of Pieces:</strong>

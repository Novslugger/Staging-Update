        <br>
        <br>
        <br>
        <br>
        <input id="edit_ancharged_ok_btn" type="button" value="Edit" />&nbsp;
</div>     

         <div id="ancharged_id_edit" style="visibility:hidden">@ViewBag.ancharged_id</div>

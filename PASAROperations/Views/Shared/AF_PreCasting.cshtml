        $('#div_load_mould_inv').css('top', '34px');
        $('#pcmould_inv_div_aflogsheet').css('height', '250px');
    });

    $('#search_pc').live('keyup', function () {

        searchin_af_act('#search_pc', '#precasting_tbl tbody tr');

    });


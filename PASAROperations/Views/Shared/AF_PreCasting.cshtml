    // $('select[id*="cmw_"].dropdown1,select[id*="cmw_"].dropdownb1').focus();
    //$('#cmw_1_1_1').focus();

    $('select[id*="cmw_"].dropdown1,select[id*="cmw_"].dropdownb1,td[id*="cmw_"].codeid').mouseover(function (e) {
        // start by setting everything to enabled


        //$('select[id*="cmw_"] option').attr('disabled', false);

        // loop each select and set the selected value to disabled in all other selects
        $('select[id*="cmw_"].dropdown1,select[id*="cmw_"].dropdownb1,td[id*="cmw_"].codeid').each(function () {
            var $this = $(this);


            $('select[id*="cmw_"].dropdown1,select[id*="cmw_"].dropdownb1').not($this).find('option').each(function () {
                if ($(this).attr('value') == $this.val() || $(this).attr('value') == $this.text())
                    $(this).attr('disabled', true);


            });
        });

    });


    load_mouldr();
    precast_customer_data();
    load_PreCast_Act_for_tab();
    load_condemned_mould();
    load_mouldreplace();

    $('.thid').hide();
    //$(".sw").hide();
    $('.codeid').hide();
    $('.div_changemould').hide();
    $('.cw1,.cw2').hide();
    $('.check_class').hide();


    //$('#mould_div_aflogsheet').hide();
    $("#idmould,#mouldclose_lnk").hide();

    $("#mould_btn,#mould_lnk").live("click", function () {
        $("#idmould,#mouldclose_lnk").show();
    });

    $("#mould_btn,#mould_lnk").live("click", function () {
        $("#mould_lnk,#mould_btn").hide();
